    c= input3.nextInt();
    
    
if((b>a) &&  (a > c)){
      
  
     maxnumber= b;
     ortnumber=a;
     minnumber=c;
  System.out.print(maxnumber );
  System.out.print(" " + ortnumber + " " );
  System.out.print(minnumber );
  
 }else if 
  ((c>a) && (a > b) ) {
    
  
      
    
    maxnumber= c;
     ortnumber=a;
     minnumber=b;
  System.out.print(maxnumber );
  System.out.print(" " + ortnumber + " " );
